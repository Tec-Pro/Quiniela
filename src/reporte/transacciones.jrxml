	<queryString>
		<![CDATA[SELECT transaccions.id, motivo, tipo, monto, fecha, usuarios.nombre as usuario, clientes.nombre as nombre, clientes.apellido as apellido, cajas.saldo as total FROM
transaccions INNER JOIN cajas ON transaccions.caja_id = cajas.id LEFT JOIN
clientes ON transaccions.cliente_id = clientes.id LEFT JOIN
usuarios ON transaccions.usuario_id = usuarios.id

where transaccions.caja_id = $P{idCaja}

Order by id]]>
	</queryString>
